<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <exclude domain="database" />
        <exclude domain="sharedpref" path="sensitive_prefs.xml" />
        <exclude domain="external" path="logs/" />
    </cloud-backup>
    <device-transfer>
        <exclude domain="database" />
        <exclude domain="external" path="sensitive/" />
    </device-transfer>
</data-extraction-rules>
